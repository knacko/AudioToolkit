<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android" >

<item android:drawable="@drawable/bg_midground_tile_border_center"/>
<item
    android:bottom="-3dp"
    android:left="-3dp"
    android:right="-3dp"
    android:top="-3dp">
    <bitmap
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:dither="true"
        android:src="@drawable/bg_midground_tile"
        android:tileMode="repeat" />

</item>
</layer-list>