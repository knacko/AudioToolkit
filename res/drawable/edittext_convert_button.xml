<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape>
            <gradient
                android:angle="270"
                android:centerY="0"
                android:endColor="#FF616161"
                android:startColor="#FF252328" />

            <corners
                android:bottomLeftRadius="5dp"
                android:bottomRightRadius="0.1dp"
                android:topLeftRadius="5dp"
                android:topRightRadius="0.1dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#FF000000" />

            <corners android:bottomLeftRadius="5dp"
                android:bottomRightRadius="0.1dp"
                android:topLeftRadius="5dp"
                android:topRightRadius="0.1dp" />

            <stroke
                android:width="3dp"
                android:color="#00424344" />
        </shape>
    </item>
    <item>
        <shape>
            <padding />

           <gradient
                android:angle="270"
                android:centerY="0"
                android:endColor="#FF2d2d2d"
                android:startColor="#FF474747" />

            <corners android:bottomLeftRadius="5dp"
                android:bottomRightRadius="0.1dp"
                android:topLeftRadius="5dp"
                android:topRightRadius="0.1dp" />

            <stroke
                android:width="6dp"
                android:color="#00424344" />
        </shape>
    </item>

